<entry name="{{ gateway_name }}-N">
            <local-address>
              <ip>
                <ipv4>{{ gateway_intf }}</ipv4>
              </ip>
              <interface>ethernet1/1</interface>
            </local-address>
            <client>
              <exclude-video-traffic>
                <applications>
                  <member>netflix-streaming</member>
                  <member>youtube-streaming</member>
                </applications>
                <enabled>yes</enabled>
              </exclude-video-traffic>
              <dns-server>
                <primary>8.8.8.8</primary>
              </dns-server>
            </client>
            <ipsec>
              <third-party-client>
                <enable>no</enable>
              </third-party-client>
            </ipsec>
            <ip-pool/>
            <tunnel-interface>{{ tunnel_name }}</tunnel-interface>
          </entry>